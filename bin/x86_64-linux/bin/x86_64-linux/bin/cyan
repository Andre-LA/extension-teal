#!/bin/sh

LUAROCKS_SYSCONFDIR='/home/runner/work/cyan-builder/cyan-builder/.luarocks/etc/luarocks' exec '/home/runner/work/cyan-builder/cyan-builder/.lua/bin/lua' -e 'package.path="/home/runner/work/cyan-builder/cyan-builder/.luarocks/share/lua/5.1/?.lua;/home/runner/work/cyan-builder/cyan-builder/.luarocks/share/lua/5.1/?/init.lua;"..package.path;package.cpath="/home/runner/work/cyan-builder/cyan-builder/.luarocks/lib/lua/5.1/?.so;"..package.cpath;local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("cyan","0.3.0-1")' '/home/runner/work/cyan-builder/cyan-builder/.luarocks/lib/luarocks/rocks-5.1/cyan/0.3.0-1/bin/cyan' "$@"
